{
  "name": "meabed/php-parallel-soap",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Meabed",
      "email": "mo.meabed@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",
  "description": "Multi curl SoapClient that allow to perform multiple requests to SoapServer",
  "homepage": "https://github.com/Meabed/php-parallel-soap",
  "keywords": [
    "Soap",
    "Curl",
    "multicurl",
    "parallel",
    "Asynchronous"
  ],
  "require": {
    "php": ">=7.0",
    "ext-soap": "*",
    "ext-curl": "*",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "fzaninotto/faker": "~1",
    "squizlabs/php_codesniffer": "~3",
    "overtrue/phplint": "~1"
  },
  "autoload": {
    "psr-4": {
      "Soap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
